<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsTrimmable>true</IsTrimmable>
    <PackageId>Biomatch.Domain</PackageId>
    <Version>0.9.0</Version>
    <Authors>Elvis Nieves Miranda</Authors>
    <Company>Puerto Rico Department of Health</Company>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/DeptSaludPR/Biomatch</RepositoryUrl>
    <Title>Biomatch Domain</Title>
    <Description>Utilities to match person records via demographic data</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LICENSE" Pack="true" PackagePath="" />
    <None Include="README.md" Pack="true" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="libphonenumber-csharp" Version="8.13.9" />
    <PackageReference Include="Quickenshtein" Version="1.5.1" />
    <PackageReference Include="SymSpell" Version="6.7.2" />
    <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="6.0.0" />
  </ItemGroup>

</Project>
